<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/image"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/image"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirstApplication">
        <activity android:name=".NavigationHost"></activity>
        <activity android:name=".RecyclerView" />
        <activity android:name=".ListView1" />
        <activity android:name=".CardViewSpinner" />
        <activity android:name=".EventHandling" />
        <activity android:name=".TableLayout" />
        <activity android:name=".FrameLayout" />
        <activity android:name=".RelativeLayout" />
        <activity android:name=".LinearLayout" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>