<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/user_form"
    app:startDestination="@id/userForm">
    <fragment
        android:id="@+id/userForm"
        android:name="com.example.firstapplication.UserForm"
        android:label="fragment_user_form"
        tools:layout="@layout/fragment_user_form" >
        <action
            android:id="@+id/action_userForm_to_userDetails2"
            app:destination="@id/userDetails2" />
    </fragment>
    <fragment
        android:id="@+id/userDetails2"
        android:name="com.example.firstapplication.UserDetails"
        android:label="fragment_user_details"
        tools:layout="@layout/fragment_user_details" >
        <action
            android:id="@+id/action_userDetails2_to_userForm"
            app:destination="@id/userForm" />
    </fragment>
</navigation>